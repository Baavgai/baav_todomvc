{
  "compilerOptions": {
    "jsx": "react",
    "moduleResolution": "node",
    "baseUrl": "src",
    "outDir": "ts_build",
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "importHelpers": true,
    "strictNullChecks": true
  },
  "include": [
    "src"
  ]    
}